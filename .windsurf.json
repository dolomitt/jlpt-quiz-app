{
  "name": "JLPT Quiz App",
  "description": "A web-based JLPT quiz application for practicing Japanese language skills",
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "testCommand": "npm test",
  "installCommand": "npm install",
  "outputDirectory": "public",
  "environment": {
    "PORT": "3000"
  },
  "tests": {
    "setup": "npm install",
    "command": "npm test",
    "e2eCommand": "npm run test:e2e",
    "outputFile": "test_results.txt",
    "timeout": 60000,
    "retries": 1
  }
}
